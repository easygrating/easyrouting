{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "removeComments": true,
    "declaration": true
  },
  "include": ["src/**/*", "test/**/*"]
}
